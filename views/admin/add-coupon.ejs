<div class="app-ecommerce">

  <!-- Add Product -->
  <div class="d-flex flex-column flex-md-row justify-content-between align-items-start align-items-md-center mb-6 gap-4 gap-md-0">

    <div class="d-flex flex-column justify-content-center">
      <h4 class="mb-1 mx-7">Add a new Coupon</h4>
      <p class="mb-0 mx-7"></p>
    </div>
    
  </div>



  <div class="container py-4">
    <div class="container mt-5">
        <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h5 class="card-title mb-0 text-primary">Coupon Information</h5>
            </div>
            <div class="card-body">
                <form action="/admin/add-coupon" method="POST">
                    <div class="row">
                        <!-- Coupon Code -->
                        <div class="mb-3 col-md-6">
                            <label for="couponCode">Coupon Code</label>
                            <input type="text" class="form-control" id="couponCode" name="coupon_code" placeholder="Coupon Code" required>
                        </div>
                        <!-- Description -->
                        <div class="mb-3 col-md-6">
                            <label for="couponDescription">Description</label>
                            <textarea class="form-control" id="couponDescription" name="description" placeholder="Description"></textarea>
                        </div>
                    </div>

                    <!-- Discount and Minimum Amount -->
                    <div class="row">
                        <div class="col-md-6">
                            <div class="mb-3">
                                <label for="discount" class="form-label">Discount (%):</label>
                                <input type="number" class="form-control" id="discount" name="discount" required>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="mb-3">
                                <label for="minAmount" class="form-label">Minimum Amount:</label>
                                <input type="number" class="form-control" id="minAmount" name="min_amount" required>
                            </div>
                        </div>
                    </div>

                    <!-- Redeem Amount and Expiry Date -->
                    <div class="row">
                        <div class="col-md-6">
                            <div class="mb-3">
                                <label for="redeemAmount" class="form-label">Redeem Amount:</label>
                                <input type="number" class="form-control" id="redeemAmount" name="redeem_amount" required>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="mb-3">
                                <label for="expiryDate" class="form-label">Expiry Date:</label>
                                <input type="date" class="form-control" id="expiryDate" name="expiryDate" required>
                            </div>
                        </div>
                    </div>

                    <!-- Status -->
                    <div class="row">
                        <div class="col-md-6">
                            <div class="mb-3">
                                <label for="couponStatus" class="form-label">Status:</label>
                                <select class="form-select" id="couponStatus" name="isListed" required>
                                    <option value="" disabled selected>Select status</option>
                                    <option value="true">Active</option>
                                    <option value="false">Inactive</option>
                                </select>
                            </div>
                        </div>
                    </div>

                    <div class="mb-3">
                        <button type="submit" class="btn btn-primary">Add Coupon</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
  
