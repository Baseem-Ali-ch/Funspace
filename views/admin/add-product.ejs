  const successParam = urlParams.get('success');

  if (successParam === 'true') {
    // Display success SweetAlert
    Swal.fire({
      icon: 'success',
      title: 'Product Added!',
      text: 'The product has been added successfully.',
      confirmButtonColor: '#3085d6',
      confirmButtonText: 'OK'
    }).then((result) => {
      // Redirect to remove the success query parameter
      if (result.isConfirmed) {
        window.location.href = '/admin/add-product';
      }
    });
  }
</script>





<script>
  function previewImage(input, imgId, replaceLinkId) {
    var imgElement = document.getElementById(imgId);
    var replaceLink = document.getElementById(replaceLinkId);
    if (input.files && input.files[0]) {
      var reader = new FileReader();
      reader.onload = function (e) {
        imgElement.src = e.target.result;
        imgElement.style.display = 'block';
        replaceLink.style.display = 'block'; // Show the replace link
      };
      reader.readAsDataURL(input.files[0]);
    } else {
      imgElement.src = "#";
      imgElement.style.display = 'none';
      replaceLink.style.display = 'none'; // Hide the replace link
    }
  }

  function triggerInputClick(inputId) {
    document.getElementById(inputId).click();
  }

  function deleteImage(inputId, previewId, replaceLinkId) {
    const input = document.getElementById(inputId);
    input.value = '';
    const preview = document.getElementById(previewId);
    preview.src = '#';
    preview.style.display = 'none';
    document.getElementById(replaceLinkId).style.display = 'none';
  }




  function triggerInputClick(id) {
    document.getElementById(id).click();
  }

  function previewImage(input, previewId) {
    if (input.files && input.files[0]) {
      const reader = new FileReader();
      reader.onload = function (e) {
        const preview = document.getElementById(previewId);
        preview.src = e.target.result;
        preview.style.display = 'block';
        document.getElementById('actions' + previewId.charAt(previewId.length - 1)).style.display = 'block';
      };
      reader.readAsDataURL(input.files[0]);
    }
  }

  function deleteImage(inputId, previewId) {
    const input = document.getElementById(inputId);
    const preview = document.getElementById(previewId);
    input.value = '';
    preview.src = '#';
    preview.style.display = 'none';
    document.getElementById('actions' + previewId.charAt(previewId.length - 1)).style.display = 'none';
  }

</script>




